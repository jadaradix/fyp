    include partials/_head-head.jade
  body
    include partials/_head-body.jade
    main
      .wrapper.section-wrapper.feature.bg-black#processing-wrapper
        section.clear-fix
          div
            div#processing
              span.hide
              div
                p
                  span.
                    &gt; Processing...
              canvas(width="640", height="480")

      .wrapper.section-wrapper.feature.bg-gray
        section.clear-fix
          div
            h1#data-headline.
            p#data-subtext.
              
    include partials/_foot-body.jade
    script(src="js/copied/jquery.min.js")
    script(src="js/copied/async.js")
    script(src="js/copied/underscore-min.js")
    script(src="js/master.js")
    script(src="js/process.js")